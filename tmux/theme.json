{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "uptime": {
      "before": "⇑ "
    },
    "branch": {
      "before": " ",
      "args": {
        "status_colors": true
      }
    },
    "virtualenv": {
      "before": "ⓔ "
    },
    "external_ip": {
      "before": "ⓦ  "
    },
    "date": {
      "before": "⌚ "
    },
    "email_imap_alert": {
      "before": "✉ ",
      "args": {
        "username": "",
        "password": ""
      }
    }
  },
  "segments": {
    "right": [
      {
        "name": "branch"
      },
      {
        "name": "virtualenv"
      },
      {
        "name": "network_load"
      },
      {
        "name": "external_ip"
      },
      {
        "name": "uptime",
        "priority": 50
      },
      {
        "name": "system_load",
        "priority": 50
      },
      {
        "name": "date"
      },
      {
        "name": "date",
        "args": {
          "format": "%H:%M",
          "istime": true
        }
      },
      {
        "name": "battery"
      },
      {
        "name": "hostname"
      }
        ]
  }
}
